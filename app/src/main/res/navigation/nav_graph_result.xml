<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_result"
    app:startDestination="@id/resultDetailsFragment">

    <fragment
        android:id="@+id/rankingFragment"
        android:name="com.example.quizyourself.JoinedQuizes.QuizResult.RankingFragment"
        android:label="fragment_ranking"
        tools:layout="@layout/fragment_ranking" >
        <argument
            android:name="quizid"
            app:argType="string" />
        <argument
            android:name="email"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/resultDetailsFragment"
        android:name="com.example.quizyourself.JoinedQuizes.QuizResult.ResultDetailsFragment"
        android:label="fragment_result_details"
        tools:layout="@layout/fragment_result_details">
        <argument
            android:name="quizid"
            app:argType="string"/>
        <argument
            android:name="email"
            app:argType="string" />
    </fragment>
</navigation>